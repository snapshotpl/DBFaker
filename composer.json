{
  "name": "snapshotpl/dbal-faker",
  "autoload-dev": {
    "psr-4": { "DBFaker\\": "tests/" }
  },
  "autoload": {
    "psr-4": { "DBFaker\\": "src/" }
  },
  "require": {
    "fzaninotto/faker": "^1.9.1",
    "doctrine/dbal": "^2.7",
    "psr/log": "^1.0",
    "php": ">=7.2",
    "mouf/utils.log.psr.errorlog_logger": "^2.0",
    "ext-bcmath": "*"
  },
  "require-dev" : {
    "symfony/console": "^3.3",
    "phpunit/phpunit": "^7.3.1",
    "squizlabs/php_codesniffer": "^3.3.1",
    "phpstan/phpstan": "^0.10.3",
    "thecodingmachine/phpstan-strict-rules": "^0.10.3",
    "thecodingmachine/dbal-fluid-schema-builder": "^1.3",
    "bamarni/composer-bin-plugin": "^1.2"
  },
  "suggest": {
    "symfony/console": "^3.3 if you want to use with symfony console"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -c phpstan.neon --level=7 --no-progress -vvv",
    "cs-fix": "phpcbf",
    "cs-check": "phpcs",
    "post-install-cmd": ["@composer bin all install --ansi"],
    "post-update-cmd": ["@composer bin all update --ansi"]
  },
  "minimum-stability" : "dev",
  "prefer-stable": true
}
